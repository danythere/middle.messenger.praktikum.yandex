{
   "compilerOptions": {
      "outDir": "outDir/",
      "baseUrl": "./src",
      "noImplicitAny": true,
      "target": "es5",
      "esModuleInterop": true,
      "noUnusedLocals": false,
      "noUnusedParameters": true,
      "sourceMap": true,
      "strictNullChecks": true
   },
   "include": [ "declaration.d.ts", "src/**/*"]
}
